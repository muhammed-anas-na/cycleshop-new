    <main class="main-wrap">
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Add Coupon</h2>
                    <p>Add, edit or delete a coupon</p>
                </div>
                <div>
                    <input type="text" placeholder="Search Coupon" class="form-control bg-white">
                </div>
            </div>
            <div class="card">
                <div class="card-body">
                    <div class="row">
                        <div class="col-md-3">
                            <form action="/admin/add-coupen" method="post">
                                <div class="mb-4">
                                    <label for="product_name" class="form-label">Name</label>
                                    <input name="couponName" pattern="^[^\s]+$" type="text" placeholder="Type here" class="form-control" id="name" required="true" maxlength="40" />
                                </div>
                                <div class="mb-4">
                                    <label class="form-label">Start Date</label>
                                    <input name="startingDate" type="date" class="form-control" id="startingDate" required="true" />    
                                </div>
                                <div class="mb-4">
                                    <label class="form-label">Expiry Date</label>
                                    <input name="exipiringDate" type="date" class="form-control" id="exipiringDate" required="true" />    
                                </div>
                                <div class="mb-4">
                                    <label for="product_name" class="form-label">Minimum Price</label>
                                    <input name="minimumPrice" type="number" placeholder="Type here" class="form-control" id="minimumPrice" required="true" maxlength="40" />
                                </div>
                                <div class="mb-4">
                                    <label for="product_name" class="form-label">Offer Price</label>
                                    <input name="offerPrice" type="number" placeholder="Type here" class="form-control" id="offerPrice" required="true" maxlength="40" />
                                </div>
                                <div class="d-grid">
                                    <button onclick="validate()" class="btn btn-primary" type="submit">Create Coupen</button>
                                </div>
                                <p id="err-msg"></p>
                            </form>
                        </div>
                        <div class="col-md-9">
                            <%if(couponData){%>
                            <div class="table-responsive">
                                <table class="table table-hover">
                                    <thead>
                                        <tr>
                                            <th>No</th>
                                        
                                            <th>Coupen Name</th>
                                            <th>Coupen code</th>
                                            <th>Start Date</th>
                                            <th>Expiry Date</th>
                                            <th>Used By</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <%for(i=0;i<couponData.length;i++){%>
                                            <tr>
                                              <td><%=i+1%></td>
                                              <td><%=couponData[i].name%></td>
                                              <td><%=couponData[i].code%></td>
                                              <td><%=couponData[i].createdS%></td>
                                              <td><%=couponData[i].expiryS%></td>
                                              <td><%=couponData[i].user.length%></td>
                                            </tr>
                                            <%}%>                                       
                                    </tbody>
                                </table>
                            </div>
                            <%}%>
