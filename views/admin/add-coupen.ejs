<script>
    const startingDateInput = document.getElementById("startingDate");
    const todayDate = new Date().toISOString().slice(0, 10);
    document.getElementById('startingDate').value = todayDate
    const todayDateObj = new Date(todayDate)
    function validate(){
        alert("Hi")
        const sDate = document.getElementById('startingDate').value
        const eDate = document.getElementById('exipiringDate').value
        const sDateObj = new Date(sDate);
        const eDateObj = new Date(eDate)
        if (sDateObj > eDateObj){
            alert("Expiry date Error")
            event.preventDefault()
        }
        if(sDateObj<todayDateObj){
            alert("Starting date should be abouve todays date");
            event.preventDefault()
        }
    }
</script>
