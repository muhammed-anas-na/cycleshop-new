    <%- include('../partials/admin-header')%> 
    <main class="main-wrap">
        
        <section class="content-main">
            <div class="content-header">
                <h2 class="content-title">Sellers list</h2>
                <div>
                    <a href="#" class="btn btn-primary"><i class="material-icons md-plus"></i> Create new</a>
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="form-control">
                        </div>
                        <div class="col-lg-2 col-md-3 col-6">
                            <select class="form-select">
                                <option>Status</option>
                                <option>Active</option>
                                <option>Disabled</option>
                                <option>Show all</option>
                            </select>
                        </div>
                        <div class="col-lg-2 col-md-3 col-6">
                            <select class="form-select">
                                <option>Show 20</option>
                                <option>Show 30</option>
                                <option>Show 40</option>
                            </select>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Email</th>
                                    <th>Status</th>
                                    <th>Number</th>
                                    <th class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                <%for(i=0;i<data.length;i++){%>
                                <tr>
                                    <td width="40%">
                                        <a href="#" class="itemside">
                                            <div class="left">
                                                <%if(data[i].image){%>
                                                    <img src="/upload/profile/<%=data[i].image%>" class="img-sm img-avatar" alt="Userpic">
                                                <%}else{%>
                                                <img src="/assets/imgs/people/avatar1.jpg" class="img-sm img-avatar" alt="Userpic">
                                                <%}%>
                                            </div>
                                            <div class="info pl-3">
                                                <h6 class="mb-0 title"><%=data[i].name%></h6>
                                                <small class="text-muted">User ID: <%= data[i]._id%></small>
                                            </div>
                                        </a>
                                    </td>
                                    <td><%= data[i].email%></td>
                                    <%if(data[i].isBlocked == 0){%>
                                    <td><span class="badge rounded-pill alert-success">Active</span></td>
                                    <%}else{%>
                                        <td><span class="badge rounded-pill alert-danger">Inactive</span></td>
                                    <%}%>
                                    <td><%= data[i].number%></td>
                                    <td class="text-end">
                                        <%if(data[i].isBlocked==0){%>
                                            <a href="/admin/block-user/<%=data[i]._id%>" class="btn btn-sm btn-brand rounded font-sm mt-15">Block</a>
                                        <%}else{%>
                                            <a href="/admin/unBlock-user/<%=data[i]._id%>" class="btn btn-sm btn-brand rounded font-sm mt-15">Unblock</a>
                                        <%}%>
                                    </td>
                                </tr>
                                <%}%>

                            </tbody>
                        </table> <!-- table-responsive.// -->
                    </div>
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
        </section> <!-- content-main end// -->
